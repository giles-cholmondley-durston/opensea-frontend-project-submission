{
  "extends": ["react-app", "airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "implicit-arrow-linebreak": 0,
    "strict": 0,
    "react/require-default-props": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "jsx-a11y/href-no-hash": [0]
  },
  "env": {
    "browser": true
  }
}
